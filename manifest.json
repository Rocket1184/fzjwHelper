{
	"name": "方正教务助手",
	"version": "0.7.5",
	"manifest_version": 2,
	"description": "让繁琐的 正方教务系统 更易于使用。",
	"icons": {
		"192": "Raw/icon192.png"
	},
	"content_scripts": [
		{
			"matches": [
				"http://jxgl.hdu.edu.cn/*",
				"http://jxglteacher.hdu.edu.cn/*"
			],
			"js": [
				"CreateMenu.js",
				"Script/kcpj.js",
				"Script/pjjd.js"
			]
		},
		{
			"matches": [
				"http://jxgl.hdu.edu.cn/*",
				"http://jxglteacher.hdu.edu.cn/*"
			],
			"js": [
				"Script/DelEmptyLi.js"
			],
			"run_at": "document_idle"
		}
	],
	"web_accessible_resources": [
		"Script/DelEmptyLi.js",
		"Script/kcpj.js",
		"Script/pjjd.js"
	]
}